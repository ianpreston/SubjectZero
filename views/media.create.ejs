<h2>Create Media File</h2>

<% if (errors) { %>
    <% errors.forEach(function(e) { %>
    <strong>Error: <%= e %></strong>
    <% }); %>
<% } %>

<form action="/media/create" enctype="multipart/form-data" method="POST">
File Path: <input type="text" name="file[path]">
<br>

File: <input type="file" name="uploaded">
<br>

<input type="submit" value="Create">
</form>
